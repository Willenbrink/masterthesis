      \strng{namehash}{0882500ab002a37d4d71f017fc10e78f}
      \strng{fullhash}{0882500ab002a37d4d71f017fc10e78f}
      \strng{bibnamehash}{0882500ab002a37d4d71f017fc10e78f}
