      \strng{namehash}{f9252f0169f4af867d643bb0f2cfc0dd}
      \strng{fullhash}{f9252f0169f4af867d643bb0f2cfc0dd}
      \strng{bibnamehash}{f9252f0169f4af867d643bb0f2cfc0dd}
      \strng{authorbibnamehash}{f9252f0169f4af867d643bb0f2cfc0dd}
      \strng{authornamehash}{f9252f0169f4af867d643bb0f2cfc0dd}
